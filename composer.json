        "illuminate/support": "~5.4",
        "illuminate/database": "~5.4",
        "illuminate/events": "~5.4"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "Gtk\\EloquentTracking\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gtk\\EloquentTracking\\Tests\\": "tests/"
