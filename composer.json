    "keywords": [
        "laravel",
        "eloquent",
        "tracking"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Giang Thai Cuong",
            "email": "thaicuong.giang@gmail.com"
        }
    ],
    "require": {
